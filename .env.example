# Environment variables for testing Pyfinity
# Copy this file to .env and fill in your actual tokens

# Discord Bot Token (from https://discord.com/developers/applications)
DISCORD_TOKEN=your_discord_token_here

# Infinity Bot List API Token (from https://infinitybots.gg)
INFINITY_TOKEN=your_infinity_token_here

# Your Discord Bot's ID (the bot's Discord user ID)
BOT_ID=your_bot_discord_id_here

# Example values (replace with your actual values):
# DISCORD_TOKEN=ODc2NTI0NzE2Nzg5...
# INFINITY_TOKEN=ibl_api_token_abc123...
# BOT_ID=876524716789012345
